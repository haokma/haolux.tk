 {\r\n      setDistrict(res.data)\r\n    }).catch(error => {\r\n      console.log('error', error);\r\n    })\r\n  }\r\n  const onChangeFiled = (e) => {\r\n    const { name, value } = e.target\r\n    if (name === 'city') {\r\n      const result = city.find((city) => {\r\n        return city.Title === value\r\n      })\r\n      if (result) {\r\n        setCitySelect(result.ID)\r\n      }\r\n    }\r\n  }\r\n  useEffect(() => {\r\n    getCity()\r\n  }, [])\r\n  useEffect(() => {\r\n    getDistrict()\r\n  }, [citySelect])\r\n  if (!token) {\r\n    history.push('/dang-nhap')\r\n    return null\r\n  }\r\n  if (!cart.cartItems.length) {\r\n    return null\r\n  }\r\n  return (\r\n    <div className=\"checkout\">\r\n      <div className=\"container\">\r\n        <Formik\r\n          initialValues={initialValues}\r\n          validationSchema={validationSchema}\r\n          onSubmit={(values) => {\r\n            const { name, phone, city, district, address } = values\r\n            const newCart = cart.cartItems.map((item) => {\r\n              return {\r\n                productId: item.prod_id,\r\n                payablePrice: item.price,\r\n                purchaseQty: item.qty\r\n              }\r\n            })\r\n            console.log(cart);\r\n            const order = {\r\n              cardId: cart.id,\r\n              cart: newCart,\r\n              name,\r\n              phone,\r\n              city,\r\n              town: district,\r\n              address,\r\n              fee: 30000,\r\n              totalAmount: cart.totalPrice\r\n            }\r\n            const action = actionPostOrder(order)\r\n            dispatch(action)\r\n              .then(() => {\r\n                history.push('/')\r\n                toast.success(\"Bạn đã đặt hàng thành công\", {\r\n                  position: toast.POSITION.TOP_RIGHT\r\n                })\r\n              })\r\n          }}\r\n\r\n        >\r\n          {\r\n            formikProps => {\r\n              return (\r\n                <div className=\"checkout__content\">\r\n                  <div className=\"row\">\r\n                    <div className=\"col-xl-6\">\r\n                      <Form className=\"checkout-form\">\r\n                        <div className=\"checkout-form__title\">\r\n                          Thanh toán và giao hàng\r\n                        </div>\r\n                        <FastField\r\n                          type='text'\r\n                          component={InputField}\r\n                          name=\"name\"\r\n                          placeholder=\"Họ và tên\"\r\n                        />\r\n                        <FastField\r\n                          type='number'\r\n                          component={InputField}\r\n                          name=\"phone\"\r\n                          placeholder=\"Số điện thoại\"\r\n                        />\r\n                        <Field\r\n                          component={SelectField}\r\n                          name=\"city\"\r\n                          placeholder=\"Thành phố\"\r\n                          options={city}\r\n                          onChangeFiled={onChangeFiled}\r\n                        />\r\n                        <Field\r\n                          component={SelectField}\r\n                          name=\"district\"\r\n                          placeholder=\" Quận/Huyện\"\r\n                          options={district}\r\n                          onChangeFiled={onChangeFiled}\r\n                        />\r\n                        <FastField\r\n                          type='text'\r\n                          component={InputField}\r\n                          name=\"address\"\r\n                          placeholder=\"Địa chỉ\"\r\n                        />\r\n                      </Form>\r\n                    </div>\r\n                    <div className=\"col-xl-6\">\r\n                      <div className=\"checkout-order\">\r\n                        <h2 className=\"checkout-order__title\">\r\n                          Đơn hàng của bạn\r\n                        </h2>\r\n                        <ul className=\"order-list\">\r\n                          <li className=\"order-list__item\">\r\n                            <span>Sản phẩm</span>\r\n                            <span>Tạm tính</span>\r\n                          </li>\r\n                          {cart.cartItems.map((product, index) => {\r\n                            return (\r\n                              <OderItem\r\n                                product={product}\r\n                                key={index}\r\n                              />\r\n                            )\r\n                          })}\r\n                          <li className=\"order-list__item\">\r\n                            <span>Tạm tính</span>\r\n                            <span>{ConvertVnd(cart.totalPrice)}</span>\r\n                          </li>\r\n                          <li className=\"order-list__item\">\r\n                            <span>Giao hàng</span>\r\n                            <span>{ConvertVnd(30000)}</span>\r\n                          </li>\r\n                          <li className=\"order-list__item\">\r\n                            <span>Tổng</span>\r\n                            <span>{ConvertVnd(cart.totalPrice + 30000)}</span>\r\n                          </li>\r\n                        </ul>\r\n                        <ul className=\"checkout-payment\">\r\n                          <li className=\"checkout-payment__item\">\r\n                            <input\r\n                              type=\"radio\"\r\n                              name=\"payment_method\"\r\n                              value=\"cod\"\r\n                              checked\r\n                              onChange={() => handleChangePayment()}\r\n                            />\r\n                            <label htmlFor=\"\">\r\n                              Thanh toán khi nhận hàng\r\n                    </label>\r\n                          </li>\r\n                          <li className=\"checkout-payment__item\">\r\n                            <input\r\n                              type=\"radio\"\r\n                              name=\"payment_method\"\r\n                              value=\"bacs\"\r\n                              onChange={() => handleChangePayment()}\r\n                            />\r\n                            <label htmlFor=\"\">\r\n                              Chuyển khoản ngân hàng\r\n                    </label>\r\n                          </li>\r\n                        </ul>\r\n                        <div className=\"checkout-button\">\r\n                          <button type=\"submit\"\r\n                            onClick={() => formikProps.handleSubmit()}\r\n                          >\r\n                            Đặt hàng\r\n                          </button>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              )\r\n            }\r\n          }\r\n        </Formik >\r\n      </div >\r\n    </div >\r\n  )\r\n}\r\n\r\nexport default CheckoutPage\r\n","C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\LoginPage\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\HomPage\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\NotFoundPage\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\CartPage\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\RegisterPage\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\ProductDetailsPage\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\actions\\userAction.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\constants\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\Header\\HeaderTop\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\ProductItem\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\Header\\HeaderNav\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\MenuMobile\\SubMenu\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\utils\\convertVnd.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\utils\\scrollToTop.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\Skeleton\\Skeleton.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\LoadingButton\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\Banner\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\Header\\HeaderNav\\DropdownItem.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\components\\MenuMobile\\SubMenu\\DropdownItem.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\ShopPage\\components\\Loading\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\ShopPage\\components\\Sidebar\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\ShopPage\\components\\Filter\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\CheckoutPage\\components\\OderItem\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\HomPage\\components\\Sale\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\HomPage\\components\\Service\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\HomPage\\components\\Promo\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\HomPage\\components\\ProductSale\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\HomPage\\components\\ProductNew\\index.js",[],"C:\\Users\\Admin\\Desktop\\Project_Ecommerce\\Server\\Client\\src\\containers\\CartPage\\components\\CartItem\\index.js",["557"],"import React, { useState } from 'react'\r\nimport { useDispatch } from 'react-redux';\r\nimport { Link } from 'react-router-dom'\r\nimport { actionRemoveCart, actionUpdateCart } from '../../../../actions/cartAction';\r\nimport ConvertVnd from '../../../../utils/convertVnd';\r\nimport { toast } from 'react-toastify';\r\nimport 'react-toastify/dist/ReactToastify.css';\r\nfunction CartItem({ product }) {\r\n  const dispatch = useDispatch()\r\n  const [quantity, setQuantity] = useState(product.qty)\r\n  const [disable, setDisable] = useState(false)\r\n  const increase = () => {\r\n    setDisable(true)\r\n    const action = actionUpdateCart(product.qty + 1, product.size, product.slug)\r\n    dispatch(action)\r\n      .then(() => {\r\n        setQuantity(product.qty + 1)\r\n        setDisable(false)\r\n      })\r\n      .catch((err) => {\r\n        setDisable(false)\r\n      })\r\n  }\r\n  const decrease = () => {\r\n    if (product.qty > 1) {\r\n      setDisable(true)\r\n      const action = actionUpdateCart(product.qty - 1, product.size, product.slug)\r\n      dispatch(action)\r\n        .then(() => {\r\n          setQuantity(product.qty - 1)\r\n          setDisable(false)\r\n        })\r\n        .catch((err) => {\r\n          setDisable(false)\r\n        })\r\n    }\r\n    else {\r\n      toast.error(\"Không thể giảm số lượng\", {\r\n        position: toast.POSITION.TOP_RIGHT\r\n      })\r\n    }\r\n  }\r\n  const onRemoveCart = () => {\r